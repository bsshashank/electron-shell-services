[{
  "_id": "_design/settings",
  "version": "0.1.0",
  "views": {
    "all": {
      "map": "function mapFun (doc) { if (doc.type === 'setting') { emit (doc.id, doc) } }"
    },
    "byExtension": {
      "map": "function mapFun (doc) { if (doc.type === 'setting') { emit (doc.extensionid, doc) } }"
    }
  }
}]
